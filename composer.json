{
  "name": "otgs/youtrack",
  "bin": ["otgs-youtrack"],
  "description": "A tool for fetching data from YouTrack",
  "keywords": [
    "cli"
  ],
  "type": "library",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Andrea Sciamanna",
      "email": "andrea.s@onthegosystems.com",
      "homepage": "http://wpml.org"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "roave/security-advisories": "dev-master",
    "nepda/youtrack-client": "dev-master",
    "symfony/console": "^3.2",
    "symfony/event-dispatcher": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "phpspec/phpspec": "^3.0",
    "leanphp/phpspec-code-coverage": "^3.0",
    "lucatume/function-mocker": "~1.0"
  },
  "autoload": {
    "psr-0": {
      "": "src/"
    },
    "classmap": [
      "src/"
    ]
  }
